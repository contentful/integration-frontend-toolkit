{
  "name": "@contentful/integration-frontend-toolkit",
  "version": "1.7.1",
  "files": [
    "components",
    "sdks",
    "types"
  ],
  "main": "./index.js",
  "types": "./index.d.ts",
  "private": false,
  "dependencies": {
    "@contentful/app-sdk": "^4.24.0",
    "@contentful/f36-components": "^4.35.0",
    "@contentful/f36-tokens": "^4.0.1",
    "@emotion/css": "^11.10.6",
    "@segment/analytics-next": "^1.66.0",
    "@sentry/react": "^7.91.0",
    "chart.js": "^4.4.1",
    "fuse.js": "^7.0.0",
    "lodash": "^4.17.21",
    "react-chartjs-2": "^5.2.0",
    "usehooks-ts": "^2.9.1"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "test": "react-scripts test --detectOpenHandles --forceExit",
    "test:ci": "CI=true react-scripts test",
    "lint": "eslint --max-warnings=0 .",
    "eject": "react-scripts eject",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes --exit-once-uploaded --onlyChanged",
    "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
    "prettier": "prettier --no-editorconfig --no-error-on-unmatched-pattern --write {apps,packages,examples}/**/*.{js,jsx,ts,tsx}",
    "prepublishOnly": "npm run build",
    "update-typewriter": "npx typewriter build --update"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-a11y": "^7.0.4",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/blocks": "^7.0.2",
    "@storybook/react": "^7.0.4",
    "@storybook/react-vite": "^7.0.4",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.16.7",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "chromatic": "^6.17.3",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "semantic-release": "^21.0.2",
    "storybook": "^7.0.2",
    "typescript": "^5.3.3",
    "typewriter": "^9.1.0",
    "vite": "^4.5.5",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-linter": "^2.0.2",
    "vite-tsconfig-paths": "^4.2.0",
    "web-vitals": "^2.1.4"
  },
  "overrides": {
    "react-scripts": {
      "typescript": "^5"
    }
  },
  "peerDependencies": {
    "@contentful/react-apps-toolkit": "^1.0.0",
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "lint-staged": {
    "*.{t,j}s?(x)": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/contentful/integration-frontend-toolkit.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  }
}
